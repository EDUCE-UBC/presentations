\mode<presentation>

\definecolor{ubcblue}{RGB}{00,28,59}

\setbeamercolor{normal text}{fg=black,bg=white}
\setbeamercolor{structure}{fg=black}
\setbeamercolor{title}{bg=ubcblue, fg=white}
\setbeamercolor{frametitle}{bg=ubcblue, fg=white}
\setbeamercolor{alerted text}{fg=green}
\setbeamercolor{item projected}{use=item,fg=black,bg=item.fg!75}

\setbeamercolor*{palette primary}{fg=black, bg=ubcblue}
\setbeamercolor*{palette secondary}{parent=palette primary,use=palette primary, fg=black, bg=palette primary.bg!95!black}
\setbeamercolor*{palette tertiary}{parent=palette primary,use=palette primary, fg=black, bg=palette primary.bg!90!black}
\setbeamercolor*{palette quaternary}{parent=palette primary,use=palette primary,fg=black,bg=palette primary.bg!85!black}

\setbeamercolor*{sidebar}{parent=palette primary}
\setbeamercolor*{palette sidebar primary}{fg=black}
\setbeamercolor*{palette sidebar secondary}{fg=black}
\setbeamercolor*{palette sidebar tertiary}{fg=black}
\setbeamercolor*{palette sidebar quaternary}{fg=black}

\setbeamercolor{framesubtitle}{fg=black}
\setbeamercolor*{subtitle}{fg=black}

\setbeamercolor*{block title}{bg=ubcblue, fg=white}
\setbeamercolor*{block title alerted}{parent=alerted text}
\setbeamercolor*{block title example}{parent=example text}

\setbeamercolor*{block body}{fg=black}
\setbeamercolor*{block body alerted}{fg=black}
\setbeamercolor*{block body example}{fg=black}

\hypersetup{linkcolor=blue}

 \setbeamertemplate{footline}{
 	\fontsize{9}{11}\selectfont%
       \hspace{10pt}\includegraphics[height=0.7cm]{images/UBC.png}%
        \hfill%
        \text{Slides}\text{ at} \url{bit.ly/ASM_EDUCE}
        \hfill%
        \usebeamercolor[fg]{page number in head/foot}%
        \usebeamerfont{page number in head/foot}%
        \insertframenumber\hspace{10pt}
        \vspace{10pt}
 }


